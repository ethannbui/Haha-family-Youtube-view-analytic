        "# Create new columns\n",
        "df[\"titles\"] = \"other\"  # default\n",
        "\n",
        "df.loc[df[\"title\"].str.lower().str.startswith(\"gia\"), \"titles\"] = \"haha_family\"\n",
        "\n",
        "df.loc[df[\"title\"].str.lower().str.startswith((\"Chuy\",\"chuyen\", \"chuyện\")),\"titles\" ] = \"mini_stories\"\n"
      ],
      "metadata": {
        "id": "dUpWMdCdewuF"
      },
      "execution_count": 7,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Extract number from title\n",
        "import re\n",
        "\n",
        "df[\"episode\"] = df[\"title\"].str.extract(r\"Tập\\s*(\\d+\\+?)\", flags=re.IGNORECASE)\n"
      ],
      "metadata": {
        "id": "eV2lBIJUh0rP"
      },
      "execution_count": 8,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Warpse 2 columns\n",
        "cols = list(df.columns)\n",
        "\n",
        "# Move titles & episode next to title\n",
        "title_index = cols.index(\"title\")\n",
        "cols.insert(title_index + 1, cols.pop(cols.index(\"titles\")))\n",
        "cols.insert(title_index + 2, cols.pop(cols.index(\"episode\")))\n",
        "\n",
        "df = df[cols]\n"
      ],
      "metadata": {
        "id": "_CO-TzLBiC78"
      },
      "execution_count": 9,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "\n",
        "# 1) Numeric episode for logic (keeps episode like '1+')\n",
        "df[\"episode_num\"] = (\n",
        "    df[\"episode\"].astype(str)\n",
        "    .str.extract(r\"(\\d+)\")\n",
        "    .astype(int)\n",
        ")\n",
        "\n",
        "# 2) Initialize\n",
        "df[\"trip_number\"] = np.nan\n",
        "\n",
        "# haha_family trip rules\n",
        "mask_haha = df[\"titles\"].eq(\"haha_family\")\n",
        "\n",
        "df.loc[mask_haha & df[\"episode_num\"].between(1, 4), \"trip_number\"] = 1\n",
        "df.loc[mask_haha & df[\"episode_num\"].between(5, 7), \"trip_number\"] = 2\n",
        "df.loc[mask_haha & df[\"episode_num\"].between(8, 11), \"trip_number\"] = 3\n",
        "df.loc[mask_haha & df[\"episode_num\"].between(12, 15), \"trip_number\"] = 4\n",
        "\n",
        "# mini_stories trip rules\n",
        "mask_mini = df[\"titles\"].eq(\"mini_stories\")\n",
        "\n",
        "df.loc[mask_mini & df[\"episode_num\"].between(1, 3), \"trip_number\"] = 1\n",
        "df.loc[mask_mini & df[\"episode_num\"].between(4, 6), \"trip_number\"] = 2\n",
        "df.loc[mask_mini & (df[\"episode_num\"].between(7, 10) | df[\"episode_num\"].eq(16)), \"trip_number\"] = 3\n",
        "df.loc[mask_mini & df[\"episode_num\"].between(11, 15), \"trip_number\"] = 4\n",
        "\n",
        "# 3) Trip name mapping\n",
        "trip_map = {\n",
        "    1: \"Ban_Lien\",\n",
        "    2: \"Sa_Huynh\",\n",
        "    3: \"Ben_Tre\",\n",
        "    4: \"Dak_Lak\",\n",
        "}\n",
        "df[\"trip\"] = df[\"trip_number\"].map(trip_map)\n",
        "df[\"trip_number\"] = df[\"trip_number\"].astype(int)\n",
        "\n",
        "df = df.drop(columns=[\"title\"])"
      ],
      "metadata": {
        "id": "FSXzArcukA4m"
