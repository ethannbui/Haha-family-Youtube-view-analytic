      "source": [
        "# Setup library\n",
        "import subprocess\n",
        "import json\n",
        "import pandas as pd\n",
        "import requests\n",
        "from time import sleep"
      ],
      "metadata": {
        "id": "mbpQ5Dh-lCnb"
      },
      "execution_count": 2,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Setup for shorter code about channel in future\n",
        "PLAYLIST_URLS = [\n",
        "    \"https://youtube.com/playlist?list=PLt3LgMEKzFxz_bKONMLhVVhqGkvgmz0rY&si=HO-n6o-jUxJZz0cy\",  # main series (giadinhhaha)\n",
        "    \"https://youtube.com/playlist?list=PLw1bw2NpPQPoIle7s972t5k22GrQkKvpO&si=6UCrErzaum8WrN9n\",         # mini series (chuyennhahaha)\n",
        "]\n",
        "\n",
        "# No limit to collect ALL videos from both playlists\n",
        "MAX_TOTAL_VIDEOS = None\n",
        "\n",
        "def run_yt_dlp(args):\n",
        "    \"\"\"\n",
        "    Run yt-dlp with arguments and return output text.\n",
        "    \"\"\"\n",
        "    result = subprocess.run(\n",
        "        [\"yt-dlp\"] + args,\n",
        "        capture_output=True,\n",
        "        text=True,\n",
        "        check=True\n",
        "    )\n",
        "    return result.stdout"
      ],
      "metadata": {
        "id": "adQfNjtClBJ-"
      },
      "execution_count": 3,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "video_ids = []\n",
        "seen = set()\n",
        "\n",
        "# Loop to run both playlist\n",
        "for plist in PLAYLIST_URLS:\n",
        "    print(f\"Fetching videos from: {plist}\")\n",
        "    playlist_json = run_yt_dlp([\"-J\", \"--flat-playlist\", plist])\n",
        "    playlist_data = json.loads(playlist_json)\n",
        "\n",
        "    for entry in playlist_data.get(\"entries\", []):\n",
        "        vid = entry.get(\"id\")\n",
        "        if vid and vid not in seen:\n",
        "            seen.add(vid)\n",
        "            video_ids.append(vid)\n",
        "\n",
        "# Recheck total video\n",
        "print(f\"Total unique videos found: {len(video_ids)}\")\n",
        "\n",
        "# No max limit → this block does nothing (optional can change with limit)\n",
        "if MAX_TOTAL_VIDEOS is not None:\n",
        "    video_ids = video_ids[:MAX_TOTAL_VIDEOS]\n",
        "    print(f\"Limiting to first {len(video_ids)} videos\")\n",
        "\n",
        "video_ids[:10]\n"
      ],
      "metadata": {
